SELECT * FROM Student;
SELECT * from Courses;
SELECT * from Enrollment;
SELECT students.Name, Courses.courseName, Enrollments.Grade
FROM Enrollments
JOIN students ON Enrollments.StudentID = students.StudentID
JOIN Courses ON Enrollments.courseId = Courses.courseId;

  
  
  
